{
  "name": "dockview-angular",
  "version": "4.13.1",
  "description": "Zero dependency layout manager supporting tabs, grids and splitviews",
  "keywords": [
    "splitview",
    "split-view",
    "gridview",
    "grid-view",
    "dockview",
    "dock-view",
    "grid",
    "tabs",
    "layout",
    "layout manager",
    "dock layout",
    "dock",
    "docking",
    "splitter",
    "drag-and-drop",
    "drag",
    "drop",
    "angular",
    "angular-component"
  ],
  "homepage": "https://github.com/mathuo/dockview",
  "bugs": {
    "url": "https://github.com/mathuo/dockview/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathuo/dockview.git"
  },
  "license": "MIT",
  "author": "https://github.com/mathuo",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:angular": "ng-packagr -c tsconfig.lib.json -p ng-package.json",
    "build:css": "node scripts/copy-css.js",
    "build": "npm run build:angular && npm run build:css",
    "clean": "rimraf dist/ .build/ .rollup.cache/",
    "rebuild": "npm run clean && npm run build",
    "test": "jest --selectProjects dockview-angular",
    "test:cov": "jest --selectProjects dockview-angular --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "dockview-core": "^4.13.1"
  },
  "peerDependencies": {
    "@angular/common": ">=21.0.6",
    "@angular/core": ">=21.0.6",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "jest-preset-angular": "^14.6.1",
    "ng-packagr": "^17.0.0",
    "zone.js": "^0.15.1"
  }
}
